		<typeAlias alias="CommentDTO"           type="com.gdu.app13.domain.CommentDTO"/> 
		<typeAlias alias="GoodDTO"              type="com.gdu.app13.domain.GoodDTO"/> 
		<typeAlias alias="MemberDTO"            type="com.gdu.app13.domain.MemberDTO"/> 
		<typeAlias alias="SummernoteImageDTO"   type="com.gdu.app13.domain.SummernoteImageDTO"/> 
	</typeAliases>
	
	









</configuration>